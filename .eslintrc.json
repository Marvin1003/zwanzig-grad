{
    "extends": "airbnb",
    "globals": {
      "TimelineLite" : false,
      "TweenLite" : false,
      "Back" : false,
      "Bounce" : false,
      "CustomEase": false,
      "Circ" : false,
      "Cubic" : false,
      "Ease" : false,
      "EaseLookup" : false,
      "Elastic" : false,
      "Expo" : false,
      "Linear" : false,
      "Power0" : false,
      "Power1" : false,
      "Power2" : false,
      "Power3" : false,
      "Power4" : false,
      "Quad" : false,
      "Quart" : false,
      "Quint" : false,
      "RoughEase" : false,
      "Sine" : false,
      "SlowMo" : false,
      "SteppedEase" : false,
      "Strong" : false,
      "Draggable" : false,
      "SplitText" : false,
      "VelocityTracker" : false, 
      "CSSPlugin" : false,
      "ThrowPropsPlugin" : false, 
      "BezierPlugin" : false
    },
    "rules": {
        "no-console": "warn",
        "comma-dangle": 0,
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/prefer-stateless-function" : 1,
        "react/react-in-jsx-scope": "off",
        "max-len": "off",
        "curly": "off",
        "no-plusplus": "off",
        "no-param-reassign": "off",
        "no-return-assign": "off",
        "arrow-parens": "off",
        "react/prop-types": [1],
        "no-underscore-dangle": "off"
    },
    "env": {
      "browser": 1
    }
}
